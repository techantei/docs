{
  "$schema": "https://mintlify.com/docs.json",
  "openapi": "/api-reference/openapi.json",
  "theme": "willow",
  "name": "Antei Taxation Documentation",
  "appearance": {
    "default": "light",
    "strict": true
  },
  "colors": {
    "primary": "#05347E",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Abra ka Dabra",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
